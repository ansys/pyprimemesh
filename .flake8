[flake8]
exclude = venv, __init__.py, doc/_build
select = W191, W291, W293, W391, E115, E117, E122, E124, E125, E225, E231, E301, E303, E501, F401, F403
count = True
max-complexity = 10
max-line-length = 100
statistics = True
per-file-ignores = 
    ansys/meshing/prime/autogen/*: F401, F403, E501
    ansys/meshing/prime/params/primestructs.py: F401, F403
    ansys/meshing/prime/internals/error_handling.py: E501
